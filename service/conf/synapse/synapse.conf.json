{
  "services": {
    "httpservice": {
      "default_servers": [],
      "discovery": {
        "method": "zookeeper",
        "path": "/nerve/services/http_service/services",
        "hosts": [
          "192.168.0.131:2181"
        ]
      },
      "haproxy": {
        "port": 3213,
        "server_options": "check inter 2s rise 3 fall 2",
        "listen": [
          "mode http",
          "option httpchk /health",
          "http-check expect string OK"
        ]
      }
    },
    "httpfrontend": {
      "default_servers": [],
      "discovery": {
        "method": "zookeeper",
        "path": "/nerve/services/http_frontend/services",
        "hosts": [
          "192.168.0.131:2181"
        ]
      },
      "haproxy": {
        "port": 3213,
        "server_options": "check inter 2s rise 3 fall 2",
        "listen": [
          "mode http",
          "option httpchk /health",
          "http-check expect string OK"
        ]
      }
    }
  },
  "haproxy": {
    "reload_command": "supervisorctl restart haproxy",
    "config_file_path": "/etc/haproxy/haproxy.cfg",
    "socket_file_path": "/var/haproxy/stats.sock",
    "do_writes": true,
    "do_reloads": true,
    "do_socket": false,
    "global": [
      "user haproxy",
      "group haproxy",
      "maxconn 4096",
      "log     127.0.0.1 local0",
      "log     127.0.0.1 local1 notice",
    ],
    "defaults": [
      "log      global",
      "option   dontlognull",
      "maxconn  2000",
      "retries  3",
      "timeout  connect 5s",
      "timeout  client  1m",
      "timeout  server  1m",
      "option   redispatch",
      "balance  roundrobin"
    ],
    "extra_sections": {
      "listen stats :3215": [
        "mode http",
        "stats enable",
        "stats uri /",
        "stats refresh 5s"
      ]
    }
  }
}